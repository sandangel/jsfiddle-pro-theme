{
  "tokenColors": [
    {
      "name": "DarkGrey",
      "scope": [
        // markup text
        "text.html",
        "text.xml",

        // source text
        "source",

        // fields, properties, members of data structure
        "variable.other.member",

        // module type like those with import * as Module from 'test';
        "entity.name.type.module",

        // property for selector in css like language
        "support.type.property-name",

        // properties access by this keyword or an object itself
        "variable.other.object.property",
        "variable.other.object",

        // properties for object literal
        "variable.object.property",

        // reference variable
        "variable.other.readwrite",

        // parameter in function
        "variable.parameter",

        // @ mark for decorator
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#39464E",
        "fontStyle": ""
      }
    },
    {
      "name": "LightGrey",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#969896",
        "fontStyle": ""
      }
    },
    {
      "name": "Blue",
      "scope": [
        // markup tag like <h1>
        "entity.name.tag",

        // name of namespace
        "entity.name.namespace",

        // name of function
        "entity.name.function",

        // markup underline for links
        "markup.underline",

        // label using in for loop
        "entity.name.label",

        // name of variable
        "entity.name.constant",
        "variable.other.constant",
        "punctuation.definition.variable",

        // selector for css, scss
        "meta.selector",

        // methods called
        "support.function",

        // properties when access object
        "support.variable.property",
        "variable.other.property",

        // name of function when reference
        "variable.function"
      ],
      "settings": {
        "foreground": "#2795EE",
        "fontStyle": ""
      }
    },
    {
      "name": "DarkBlue",
      "scope": [
        // type built into the language like null, undefined
        "constant.language",

        // framework bultin constant, variable, type like __dirname in nodejs or css value
        "support.constant",
        "support.variable",
        "support.type.builtin",
        "support.type.primitive",

        // color value
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#221199",
        "fontStyle": ""
      }
    },
    {
      "name": "Purple",
      "scope": [
        "keyword",

        // markup *words* in markdown
        "markup.italic",

        // language keyword like function, class...
        "storage.type",

        // React Component attributes.
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.js",
        "entity.other.attribute-name.jsx",

        // class, member modifier like static, private
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#a151d2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Orange",
      "scope": [
        // markup attribute
        "entity.other.attribute-name",

        // markup **words** in markdown
        "markup.bold",

        // inherit class
        "entity.other.inherited-class",

        // escape character in string
        "constant.character.escape",

        // placeholder for string
        "constant.other.placeholder",

        // data structures name
        "entity.name",
        "entity.name.class",
        "entity.name.class.forward-decl",
        "entity.name.type",
        "entity.name.struct",
        "entity.name.enum",
        "entity.name.union",
        "entity.name.trait",
        "entity.name.interface",

        // type, interface for type language like graphql, dart
        "support.class",
        "support.type",

        // markup header like in markdown
        "entity.name.section",
        "markup.heading",

        // import, export variables
        "variable.other.readwrite.alias",

        // string interpolation mark
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#f18c16",
        "fontStyle": ""
      }
    },
    {
      "name": "Green",
      "scope": [
        "string",

        // markup quote
        "markup.quote",

        // type parameter mark
        "punctuation.definition.typeparameters",

        // query parameter in graphql
        "variable.parameter.graphql"
      ],
      "settings": {
        "foreground": "#249D7F",
        "fontStyle": ""
      }
    },
    {
      "name": "Red",
      "scope": [
        // numberic value
        "constant.numeric",

        // operator
        "keyword.operator",

        // illegal or old version
        "invalid",

        // type seperator '|'
        "punctuation.or",

        // reserved variables such as this, self, super
        "variable.language"
      ],
      "settings": {
        "foreground": "#ED5C65",
        "fontStyle": ""
      }
    }
  ],
  "colors": {
    "focusBorder": "#705697AA",
    "pickerGroup.foreground": "#705697AA",
    "pickerGroup.border": "#705697",
    "list.focusBackground": "#d7d4f0f1",
    "list.hoverBackground": "#a69fe928",
    "list.activeSelectionForeground": "#6c6c6c",
    "list.activeSelectionBackground": "#a69fe96c",
    "list.inactiveSelectionBackground": "#a69fe941",
    "list.highlightForeground": "#9769dc",
    "selection.background": "#d7d4f0f1",
    "editor.background": "#F5F5F5",
    "editorWhitespace.foreground": "#AAAAAA",
    "editor.lineHighlightBackground": "#a69fe941",
    "editor.selectionHighlightBackground": "#d7d4f081",
    "editor.wordHighlightBackground": "#d7d4f081",
    "editor.wordHighlightStrongBackground": "#d7d4f0f1",
    "editorLineNumber.activeForeground": "#9769dc",
    "editor.selectionBackground": "#d7d4f0f1",
    "editorBracketMatch.background": "#F5F5F5",
    "editorBracketMatch.border": "#9769dc",
    "panel.background": "#F5F5F5",
    "sideBar.background": "#F2F2F2",
    "sideBarSectionHeader.background": "#ede8ef",
    "editorLineNumber.foreground": "#9DA39A",
    "editorCursor.foreground": "#54494B",
    "inputOption.activeBorder": "#adafb7",
    "dropdown.background": "#F5F5F5",
    "editor.findMatchBackground": "#BF9CAC",
    "editor.findMatchHighlightBackground": "#edc9d8",
    "peekViewEditor.matchHighlightBackground": "#C2DFE3",
    "peekViewTitle.background": "#F2F8FC",
    "peekViewEditor.background": "#F2F8FC",
    "peekViewResult.background": "#F2F8FC",
    "peekView.border": "#705697",
    "peekViewResult.matchHighlightBackground": "#93C6D6",
    "statusBar.background": "#705697",
    "statusBar.noFolderBackground": "#705697",
    "statusBar.debuggingBackground": "#9769dc",
    "activityBar.background": "#EDEDF5",
    "activityBar.foreground": "#705697",
    "activityBarBadge.background": "#705697",
    "titleBar.activeBackground": "#a69fe941",
    "scrollbarSlider.background": "#d7d4f0f1",
    "button.background": "#705697",
    "editorGroup.dropBackground": "#C9D0D988",
    "inputValidation.infoBorder": "#4ec1e5",
    "inputValidation.infoBackground": "#f2fcff",
    "inputValidation.warningBackground": "#fffee2",
    "inputValidation.warningBorder": "#ffe055",
    "inputValidation.errorBackground": "#ffeaea",
