{
  "tokenColors": [
    {
      "name": "DarkGrey",
      "scope": [
        "variable.other.property",

        // ts
        "variable.other.object.property.ts",

        // css
        "support.type.property-name.media.css",
        "variable.function"
      ],
      "settings": {
        "foreground": "#39464E"
      }
    },
    {
      "name": "LightGrey",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#969896"
      }
    },
    {
      "name": "Blue",
      "scope": [
        "entity.name.type",
        "entity.name.tag",
        "variable.other.object",
        "entity.other.inherited-class",
        "entity.name.function",
        "variable.parameter",
        "variable.other.readwrite",
        "support.class.console",

        // js
        "support.function.js",

        // dart
        "support.class.dart",

        // css
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#2795EE"
      }
    },
    {
      "name": "DarkBlue",
      "scope": [
        "constant.language.boolean",
        "constant.other.color.rgb-value",
        "support.constant.property-value",
        ""
      ],
      "settings": {
        "foreground": "#221199"
      }
    },
    {
      "name": "Purple",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "storage.type.class",
        "storage.modifier",
        "variable.language.this",
        "keyword.control",
        "variable.parameter",

        // ts
        "keyword.control.flow.ts",

        // dart
        "keyword.other.import.dart",
        "keyword.declaration.dart",

        // css
        "support.constant.color.w3c-standard-color-name.css"
      ],
      "settings": {
        "foreground": "#a151d2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Orange",
      "scope": [
        "entity.name",
        "variable.other.readwrite.alias",
        "string.quoted",
        "variable.parameter",
        "keyword.control.at-rule.import",
        "keyword.control.at-rule.media",

        // js
        "constant.language.boolean.js",
        "variable.other.constant.property.js",

        // html
        "entity.other.attribute-name.html",

        // css
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css",

        // dart
        "storage.type.primitive.dart",
        "storage.type.annotation.dart",
        "variable.language.dart",

        // json
        "constant.language.json"
      ],
      "settings": {
        "foreground": "#f18c16"
      }
    },
    {
      "name": "Green",
      "scope": [
        "string.quoted",
        "support.type.primitive",

        // dart
        "string.interpolated.dart"
      ],
      "settings": {
        "foreground": "#249D7F"
      }
    },
    {
      "name": "Red",
      "scope": ["constant.numeric", "keyword.other.unit", "keyword.operator"],
      "settings": {
        "foreground": "#ED5C65"
      }
    }
  ],
  "colors": {
    "editor.background": "#F3F5F6",
    "editor.foreground": "#39464E",
    "editorCursor.foreground": "#39464E",
    "editorWhitespace.foreground": "#969896",
    "editorIndentGuide.background": "#AAAAAA",
    "editor.lineHighlightBackground": "#D7D4F030",
    "editor.rangeHighlightBackground": "#FDFF00",
    "editor.hoverHighlightBackground": "#ADD6FF",
    "peekViewEditor.matchHighlightBackground": "#F5D802",
    "editor.selectionBackground": "#D7D4F0",
    "editor.inactiveSelectionBackground": "#E1E5EA",
    "editor.findRangeHighlightBackground": "#D7D4F040",
    "editor.findMatchHighlightBackground": "#D7D4F060",
    "peekViewResult.matchHighlightBackground": "#D7D4F060",
    "editor.findMatchBackground": "#D7D4F080",
    "editor.wordHighlightBackground": "#D7D4F050",
    "editor.wordHighlightStrongBackground": "#D7D4F070"
  },
  "name": "JSFiddle Pro"
}
