{
  "tokenColors": [
    {
      "name": "DarkGrey",
      "scope": [
        // markup text
        "text.html",
        "text.xml",

        // source text
        "source",

        // fields, properties, members of data structure
        "variable.other.member",

        // module type like those with import * as Module from 'test';
        "entity.name.type.module",

        // property for selector in css like language
        "support.type.property-name",

        // properties access by this keyword or an object itself
        "variable.other.object.property",
        "variable.other.object",

        // properties for object literal
        "variable.object.property",

        // reference variable
        "variable.other.readwrite",

        // parameter in function
        "variable.parameter",

        // @ mark for decorator
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#39464E",
        "fontStyle": ""
      }
    },
    {
      "name": "LightGrey",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#969896",
        "fontStyle": ""
      }
    },
    {
      "name": "Blue",
      "scope": [
        // markup tag like <h1>
        "entity.name.tag",

        // name of namespace
        "entity.name.namespace",

        // name of function
        "entity.name.function",

        // markup underline for links
        "markup.underline",

        // label using in for loop
        "entity.name.label",

        // name of variable
        "entity.name.constant",
        "variable.other.constant",
        "punctuation.definition.variable",

        // selector for css, scss
        "meta.selector",

        // methods called
        "support.function",

        // access properties of variable
        "support.variable.property",

        // properties when access object
        "variable.other.property",

        // name of function when reference
        "variable.function"
      ],
      "settings": {
        "foreground": "#2795EE",
        "fontStyle": ""
      }
    },
    {
      "name": "DarkBlue",
      "scope": [
        // type built into the language like null, undefined
        "constant.language",

        // framework bultin constant, variable, type like __dirname in nodejs or css value
        "support.constant",
        "support.variable",
        "support.type.builtin",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#221199",
        "fontStyle": ""
      }
    },
    {
      "name": "Purple",
      "scope": [
        "keyword",

        // markup italic
        "markup.italic",

        // language keyword like function, class...
        "storage.type",

        // class, member modifier like static, private
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#a151d2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Orange",
      "scope": [
        // markup attribute
        "entity.other.attribute-name",

        // markup bold
        "markup.bold",

        // inherit class
        "entity.other.inherited-class",

        // escape character in string
        "constant.character.escape",

        // placeholder for string
        "constant.other.placeholder",

        // data structures name
        "entity.name",
        "entity.name.class",
        "entity.name.class.forward-decl",
        "entity.name.type",
        "entity.name.struct",
        "entity.name.enum",
        "entity.name.union",
        "entity.name.trait",
        "entity.name.interface",

        // type, interface for type language like graphql, dart
        "support.class",
        "support.type",

        // markup header like in markdown
        "entity.name.section",
        "markup.heading",

        // import, export variables
        "variable.other.readwrite.alias",

        // string interpolation mark
        "punctuation.definition.template-expression",

        /** graphql */
        // query parameter
        "variable.parameter.graphql"
      ],
      "settings": {
        "foreground": "#f18c16",
        "fontStyle": ""
      }
    },
    {
      "name": "Green",
      "scope": [
        "string",

        // markup quote
        "markup.quote",

        // type parameter mark
        "punctuation.definition.typeparameters"
      ],
      "settings": {
        "foreground": "#249D7F",
        "fontStyle": ""
      }
    },
    {
      "name": "Red",
      "scope": [
        // numberic value
        "constant.numeric",

        // operator
        "keyword.operator",

        // illegal or old version
        "invalid",

        // type seperator '|'
        "punctuation.or",

        // reserved variables such as this, self, super
        "variable.language"
      ],
      "settings": {
        "foreground": "#ED5C65",
        "fontStyle": ""
      }
    }
  ],
  "colors": {
    "editor.background": "#F3F5F6",
    "editor.foreground": "#39464E",
    "editorCursor.foreground": "#39464E",
    "editorWhitespace.foreground": "#969896",
    "editorIndentGuide.background": "#AAAAAA",
    "editor.lineHighlightBackground": "#D7D4F0",
    "editor.rangeHighlightBackground": "#FDFF00",
    "editor.hoverHighlightBackground": "#ADD6FF",
    "peekViewEditor.matchHighlightBackground": "#F5D802",
    "editor.selectionBackground": "#D7D4F0",
    "editor.inactiveSelectionBackground": "#E1E5EA",
    "editor.findRangeHighlightBackground": "#D7D4F040",
    "editor.findMatchHighlightBackground": "#D7D4F060",
    "peekViewResult.matchHighlightBackground": "#D7D4F060",
    "editor.findMatchBackground": "#D7D4F080",
    "editor.wordHighlightBackground": "#D7D4F050",
    "editor.wordHighlightStrongBackground": "#D7D4F070"
  },
  "name": "JSFiddle Pro"
}
